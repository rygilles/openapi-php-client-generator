{
    "name": "rygilles/openapi-php-client-generator",
    "description": "Generate PHP client library from OpenAPI schema.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "OpenAPI",
        "Schema",
        "PHP",
        "Generator"
    ],
    "homepage": "http://github.com/rygilles/openapi-php-client-generator",
    "authors": [
        {
            "name": "Ryan Gilles",
            "email": "ryan@e-monsite.com"
        }
    ],
    "minimum-stability": "dev",
    "bin": [
        "open-api-client-generator"
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/console": "~2.3|~3.0",
        "symfony/filesystem": "~2.3|~3.0",
        "symfony/process": "~2.3|~3.0"
    },
    "autoload": {
        "psr-0": {
            "Rygilles\\OpenApiPhpClientGenerator": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rygilles\\OpenApiPhpClientGenerator\\Tests\\": "tests/"
        }
    }
}
