{
    "name": "rygilles/openapi-php-client-generator",
    "description": "Generate PHP client library from OpenAPI schema.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "OpenAPI",
        "Schema",
        "PHP",
        "Generator"
    ],
    "homepage": "http://github.com/rygilles/openapi-php-client-generator",
    "authors": [
        {
            "name": "Ryan Gilles",
            "email": "ryan@e-monsite.com"
        }
    ],
    "minimum-stability": "dev",
    "bin": [
        "openapi-php-client-generator"
    ],
    "require": {
        "php": ">=7.4",
        "symfony/console": "6.0.x-dev",
        "symfony/filesystem": "6.0.x-dev",
        "symfony/process": "6.0.x-dev",
        "symfony/yaml": "6.0.x-dev",
        "twig/twig": "3.x-dev"
    },
    "autoload": {
        "psr-0": {
            "Rygilles\\OpenApiPhpClientGenerator": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rygilles\\OpenApiPhpClientGenerator\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/var-dumper": "6.0.x-dev"
    }
}
